# Database (NeonDB)
DATABASE_URL="postgresql://user:password@host/database?sslmode=require"

# API Server
PORT=3001

# JWT Authentication (minimum 32 characters)
JWT_SECRET="your-super-secret-jwt-key-min-32-chars"

# Anthropic API
ANTHROPIC_API_KEY="your-anthropic-api-key"

